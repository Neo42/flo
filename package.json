{
  "name": "slick",
  "version": "1.0.0",
  "scripts": {
    "frontend:dev": "webpack serve --liveReload --progress --config ./webpack/webpack.dev.js",
    "frontend:build": "webpack --progress --config ./webpack/webpack.prod.js",
    "backend:dev": "nodemon backend.js",
    "backend:build": "node backend.js",
    "start": "run-p frontend:dev backend:dev",
    "prepare": "husky install"
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "husky": "^6.0.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "include-media": "^1.4.10",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.1.0",
    "prettier": "2.3.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "sass-migrator": "^1.5.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prismicio/client": "^5.1.0",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "modern-css-reset": "^1.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "prismic-dom": "^2.2.5",
    "pug": "^3.0.2"
  }
}
